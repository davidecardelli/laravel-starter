{
    "name": "laravel-template",
    "description": "Production-ready Laravel 12 template with Vue 3, TypeScript, and complete quality tooling",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vue-tsc --noEmit && vite build",
        "build:ssr": "vue-tsc --noEmit && vite build && vite build --ssr",
        "dev": "vite",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint . --fix",
        "lint:check": "eslint .",
        "format": "prettier --write resources/",
        "format:check": "prettier --check resources/",
        "check": "npm run type-check && npm run lint:check && npm run format:check"
    },
    "devDependencies": {
        "@eslint/js": "^9.19.0",
        "@laravel/vite-plugin-wayfinder": "^0.1.3",
        "@tailwindcss/vite": "^4.1.11",
        "@types/node": "^24.9.2",
        "@vitejs/plugin-vue": "^6.0.0",
        "@vue/eslint-config-typescript": "^14.3.0",
        "concurrently": "^9.0.1",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-vue": "^10.5.1",
        "prettier": "^3.4.2",
        "prettier-plugin-organize-imports": "^4.1.0",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "typescript": "^5.2.2",
        "typescript-eslint": "^8.23.0",
        "vite": "^7.0.4",
        "vue-tsc": "^3.1.2"
    },
    "dependencies": {
        "@inertiajs/vue3": "^2.1.0",
        "@vueuse/core": "^14.0.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "laravel-vite-plugin": "^2.0.0",
        "lucide-vue-next": "^0.552.0",
        "reka-ui": "^2.4.1",
        "tailwind-merge": "^3.2.0",
        "tailwindcss": "^4.1.1",
        "tw-animate-css": "^1.2.5",
        "vue": "^3.5.13"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.52.5",
        "@rollup/rollup-win32-x64-msvc": "4.52.5",
        "@tailwindcss/oxide-linux-x64-gnu": "^4.0.1",
        "@tailwindcss/oxide-win32-x64-msvc": "^4.0.1",
        "lightningcss-linux-x64-gnu": "^1.29.1",
        "lightningcss-win32-x64-msvc": "^1.29.1"
    }
}
